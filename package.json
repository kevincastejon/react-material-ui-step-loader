{
  "name": "react-material-ui-step-loader",
  "description": "Stepped Progress react component using material-ui",
  "version": "1.47.0",
  "private": false,
  "main": "dist/index.js",
  "storybookPage": "https://kevincastejon.github.io/react-material-ui-step-loader/storybook",
  "docPage": "https://kevincastejon.github.io/react-material-ui-step-loader/",
  "homepage": "https://kevincastejon.github.io/react-material-ui-step-loader/demo",
  "repository": "https://github.com/kevincastejon/react-material-ui-step-loader.git",
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "transpile": "babel src/lib -d dist --copy-files",
    "prepublishOnly": "yarn transpile",
    "storybook": "start-storybook -p 9009 -s public",
    "github-page": "yarn build && npx http-server ./build/",
    "create-readme": "yarn create-react-readme ./package.json ./src/lib/components/ ./readme.md -t ./template.handlebars -u ./usage_readme.md -d ./dev_readme.md",
    "build-storybook": "build-storybook -c .storybook -o ./build/storybook",
    "postbuild": "fcli move ./build/ ./demo/ && fcli move ./demo/ ./build/demo && yarn build-storybook && fcli copy ./readme.md ./build/index.md && fcli copy ./_config.yml ./build/_config.yml",
    "pregh:deploy": "yarn build",
    "gh:deploy": "gh-pages -d build",
    "publish": "yarn publish",
    "post:publish":"yarn gh:deploy",
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "create-react-readme": "^1.0.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-cli": "^1.2.0",
    "gh-pages": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  }
}
